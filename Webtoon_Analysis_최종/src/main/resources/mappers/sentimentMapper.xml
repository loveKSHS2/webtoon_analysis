<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.webtoon.mapper.sentimentMapper">
	<select id="selectSentiment" resultType="sentimentVO">
		SELECT title,episode,positive,med,negative,positive_percent,med_percent,negative_percent FROM sentiment
	</select>
    
</mapper>